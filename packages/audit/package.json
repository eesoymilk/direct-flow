{
  "name": "@direct-flow/audit",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 3001",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:quiet": "eslint --quiet",
    "format": "prettier --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "check": "pnpm type-check && pnpm lint",
    "check:fix": "pnpm type-check && pnpm lint:fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate --config=drizzle-dev.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle-dev.config.ts"
  },
  "dependencies": {
    "@nuxt/eslint": "1.9.0",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "2.0.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "^3.3.3",
    "@pinia/nuxt": "0.11.2",
    "@vueuse/nuxt": "13.9.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "drizzle-orm": "^0.44.4",
    "nuxt": "^4.1.1",
    "nuxt-auth-utils": "0.5.20",
    "pg": "^8.16.0",
    "pinia": "^3.0.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@direct-flow/base": "workspace:*",
    "@types/pg": "^8.15.4",
    "drizzle-kit": "^0.31.4",
    "prettier": "^3.5.3",
    "tsx": "^4.20.5",
    "typescript": "^5.8.3"
  }
}
