{
  "name": "@direct-flow/company",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:quiet": "eslint --quiet",
    "format": "prettier --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "check": "pnpm type-check && pnpm lint",
    "check:fix": "pnpm type-check && pnpm lint:fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate --config=drizzle-dev.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle-dev.config.ts",
    "deploy": "pnpm dlx vercel deploy --prebuilt --prod",
    "deploy:preview": "pnpm dlx vercel deploy --prebuilt"
  },
  "dependencies": {
