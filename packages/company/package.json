{
  "name": "@direct-flow/company",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:quiet": "eslint --quiet",
    "format": "prettier --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "check": "pnpm type-check && pnpm lint",
    "check:fix": "pnpm type-check && pnpm lint:fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate --config=drizzle-dev.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle-dev.config.ts"
  },
  "dependencies": {
    "@internationalized/date": "^3.9.0",
    "nuxt": "^4.1.1",
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@direct-flow/base": "workspace:*",
    "@faker-js/faker": "^9.9.0",
    "@iconify-json/lucide": "^1.2.66",
    "@nuxt/test-utils": "^3.17.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/vue": "^8.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^13.6.0",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.31.4",
    "happy-dom": "^17.0.0",
    "jsdom": "^24.0.0",
    "playwright-core": "^1.55.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.8",
    "vitest": "^2.1.9"
  }
}
